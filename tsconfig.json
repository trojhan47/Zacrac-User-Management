{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"outDir": "dist",
		"strict": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"sourceMap": true,
		"baseUrl": ".",
		"paths": {
			"*": ["node_modules/*", "src/types/*"]
		}
	},
	"include": ["src/**/*"]
	// "exclude": ["node_modules"]
}
